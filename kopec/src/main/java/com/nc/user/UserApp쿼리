		 SELECT * FROM                                                                                                                                                                           
		 (SELECT A.YEAR AYEAR,A.MEASUREID AMID,'M' TAG FROM TBLAUTHORITY A WHERE A.USERID like '%' AND YEAR='2008'                                                                                      
		 UNION                                                                                                                                                                                   
		 SELECT D.YEAR AYEAR,D.ID AMID,'S' TAG FROM TBLMEASUREDEFINE D WHERE D.UPDATEID like '%' AND YEAR='2008') AUT                                                                                   
		 LEFT JOIN                                                                                                                                                                               
		 (SELECT T.ID MID,T.PARENTID MPID,T.CONTENTID MCID,T.TREELEVEL MLEVEL,T.RANK MRANK,T.WEIGHT MWEIGHT,C.NAME MNAME,D.FREQUENCY,T.YEAR MTYEAR,D.MEASUREMENT                                 
		 FROM TBLTREESCORE T,TBLMEASUREDEFINE D, TBLMEASURE C WHERE T.CONTENTID=D.ID AND D.MEASUREID=C.ID AND T.TREELEVEL=5 AND T.YEAR=? AND MEASUREMENT='°è·®' ) MEA                            
		 ON AUT.AMID=MEA.MCID AND AUT.AYEAR=MTYEAR                                                                                                                                               
		 LEFT JOIN                                                                                                                                                                               
		 (SELECT T.ID OID,T.PARENTID OPID,T.CONTENTID OCID,T.TREELEVEL OLEVEL,T.RANK ORANK,T.WEIGHT OWEIGHT,C.NAME ONAME                                                                         
		 FROM TBLTREESCORE T,TBLOBJECTIVE C WHERE T.CONTENTID=C.ID AND T.TREELEVEL=4 AND T.YEAR='2008') OBJ                                                                                           
		 ON MEA.MPID=OBJ.OID                                                                                                                                                                     
		 LEFT JOIN                                                                                                                                                                               
		 (SELECT T.ID PID,T.PARENTID PPID,T.CONTENTID PCID,T.TREELEVEL PLEVEL,T.RANK PRANK,T.WEIGHT PWEIGHT,C.NAME PNAME                                                                         
		 FROM TBLTREESCORE T,TBLPST C WHERE T.CONTENTID=C.ID AND T.TREELEVEL=3 AND T.YEAR='2008') PST                                                                                                 
		 ON OBJ.OPID=PST.PID                                                                                                                                                                     
		 INNER JOIN                                                                                                                                                                              
		 (SELECT T.ID BID,T.PARENTID BPID,T.CONTENTID BCID,T.TREELEVEL BLEVEL,T.RANK BRANK,T.WEIGHT BWEIGHT,C.NAME BNAME                                                                         
		 FROM TBLHIERARCHY T,TBLBSC C WHERE T.CONTENTID=C.ID AND T.TREELEVEL=2 AND T.YEAR='2008') BSC                                                                                                 
		 ON PST.PPID=BSC.BID                                                                                                                                                                     
		 INNER JOIN                                                                                                                                                                              
		 (SELECT T.ID SID,T.PARENTID SPID,T.CONTENTID SCID,T.TREELEVEL SLEVEL,T.RANK SRANK,T.WEIGHT SWEIGHT,C.NAME SNAME                                                                         
		 FROM TBLHIERARCHY T,TBLSBU C WHERE T.CONTENTID=C.ID AND T.TREELEVEL=1 AND T.YEAR='2008') SBU                                                                                                 
		 ON BSC.BPID=SBU.SID                                                                                                                                                                     
		 LEFT JOIN                                                                                                                                                                                                 
		 (SELECT ROUND(D.ACTUAL,3) ACTUAL,D.PLANNED,D.PLANNEDBASE,D.BASE,D.BASELIMIT,D.LIMIT,FILEPATH,FILENAME,D.MEASUREID,ROUND(S.SCORE,3) SCORE,S.GRADE,S.GRADE_SCORE FROM TBLMEASUREDETAIL D, TBLMEASURESCORE S 
		 WHERE D.MEASUREID=S.MEASUREID AND substr(D.STRDATE,1,6)=substr(S.STRDATE,1,6)) DET                                                                                                                        
		 ON AUT.AMID=DET.MEASUREID                                                                                                                                                                                 
		 WHERE MID IS NOT NULL ORDER BY OID,ORANK,PRANK,PID,MRANK     